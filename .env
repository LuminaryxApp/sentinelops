# =============================================================================
# Rate Limiting
# =============================================================================

# Maximum number of requests per time window (default: 100)
RATE_LIMIT_MAX=100

# Rate limit time window in milliseconds (default: 60000 = 1 minute)
RATE_LIMIT_WINDOW_MS=60000

# =============================================================================
# Logging Configuration
# =============================================================================

# Log level: trace, debug, info, warn, error, fatal (default: info)
LOG_LEVEL=info

# Enable pretty-printed logs (default: false)
# Set to 'true' for development, 'false' for production
LOG_PRETTY=false

# =============================================================================
# File Operation Limits
# =============================================================================

# Maximum file read size in MB (default: 2)
# Files larger than this will not be read
MAX_READ_SIZE_MB=2

# Maximum request body size in MB (default: 1)
MAX_BODY_SIZE_MB=1

# Maximum number of operations in a batch request (default: 50)
MAX_BATCH_OPS=50

# Maximum number of search results to return (default: 200)
MAX_SEARCH_RESULTS=200

# =============================================================================
# Desktop App Configuration
# =============================================================================

# Enable Electron DevTools in production (default: false)
# Set to 'true' to enable DevTools even in production builds
ELECTRON_DEV_TOOLS=false

# =============================================================================
# Development Configuration
# =============================================================================

